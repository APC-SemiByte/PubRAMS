@model webapp.Models.ViewModels.StaffRolesView;
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Roles</h1>
</div>

@foreach(var staffRole in Model.StaffRoles)
{
<h1 class="display-5">@(staffRole.Email)</h1>
@foreach(var role in staffRole.Roles)
{
    <h2 class="display-6">@role</h2>
}
<br/>
}


