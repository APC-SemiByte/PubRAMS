<body style="background-color: aliceblue;">
    <h1 class="greeting" style="display: flex;
            justify-content: center;
            align-items: center;
            height: 20vh;
            color: rgb(114, 169, 201)">CATALOGING</h1>

    <div class="border" style="
            border: 3px solid; border-radius: 15px;
            padding: 20px;
            box-shadow: 10px 10px 30px rgba(14, 14, 14, 0.2);
            border-color: #f5f5f5;
            background-color: rgba(255, 255, 255)">
        <form>
            <!-- PROJECT DETAILS -->
            <div class="pt-2">
                <h3>Project Details</h2>
                    <div class="py-3">
                        <div>
                            <label><strong>Project Title</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                        </div>
                        <div class="col">
                            <label>Title of Project</label>
                        </div>
                    </div>
            </div>

            <div>
                <label><strong>Abstract</strong></label>
                <li class="list-inline-item dropdown">
                    <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                        aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                            fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                            <path
                                d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                        </svg></a>
                    <div class="dropdown-menu dropdown-menu-end">
                        <a class="dropdown-item" href="#">Confirm</a>
                        <a class="dropdown-item" href="#">Edit</a>
                    </div>
                </li>
            </div>
            <div class="col">
                <label>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor
                    incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud
                    exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute
                    irure
                    dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla
                    pariatur.
                    Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt
                    mollit anim id est laborum.</label>
            </div>


            <!-- PROJECT TAGS -->
            <div class="pt-5">
                <h3>Project Tags</h2>
                    <div class="row py-3">
                        <div class="col">
                            <label><strong>School/Department</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                            <div class="col">
                                <label>School of (School)</label>
                            </div>
                        </div>
                        <div class="col">
                            <label><strong>Course</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                            <div class="col">
                                <label>Course Name</label>
                            </div>
                        </div>
                    </div>

                    <div class="row py-3">
                        <div class="col">
                            <label><strong>Subject</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                            <div class="col">
                                <label>Subject Name</label>
                            </div>
                        </div>
                        <div class="col">
                            <label><strong>Categories</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                            <div class="col">
                                <label>Subject Name</label>
                            </div>
                        </div>
                    </div>
            </div>


            <!-- PROJECT INVOLVEMENT -->
            <div class="pt-5">
                <h3>Project Involvement</h2>
                    <div class="row py-3">
                        <div class="col">
                            <label><strong>Contributor/Co-Authors</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                            <div class="row">
                                <label>Contributer/Co-Author Name 1</label>
                                <label>Contributer/Co-Author Name 2</label>
                                <label>Contributer/Co-Author Name 3</label>
                            </div>
                        </div>
                        <div class="col">
                            <label><strong>Adviser</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                            <div class="col">
                                <label>Adviser Name</label>
                            </div>
                        </div>
                    </div>

                    <div class="row py-3">
                        <div class="col">
                            <label><strong>Consultant</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                            <div class="col">
                                <label>Consultant Name</label>
                            </div>
                        </div>
                        <div class="col">
                            <label><strong>Professor</strong></label>
                            <li class="list-inline-item dropdown">
                                <a class="text-muted font-size-18 px-2" href="#" role="button" data-bs-toggle="dropdown"
                                    aria-haspopup="true"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                        fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                        <path
                                            d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0m0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                                    </svg></a>
                                <div class="dropdown-menu dropdown-menu-end">
                                    <a class="dropdown-item" href="#">Confirm</a>
                                    <a class="dropdown-item" href="#">Edit</a>
                                </div>
                            </li>
                            <div class="col">
                                <label>Professor Name</label>
                            </div>
                        </div>
                    </div>
            </div>


            <!-- PROJECT FILES -->
            <div class="pt-5 pb-2">
                <h3>Project Files</h2>
                    <div class="custom-file">
                        <input type="file" class="custom-file-input" id="customFile">
                    </div>
            </div>
        </form>
    </div>

    @* BUTTON *@
    <div class="row">
        <div class="col">
            @* --- *@
        </div>
        <div class="col">
            <div class="pt-5 float-end">
                <button type="submit" class="btn btn-primary">Confirm</button>
            </div>
        </div>

    </div>
</body>