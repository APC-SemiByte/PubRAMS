<style>
  .hover-text::after {
    content: "";
    position: absolute;
    background-color: rgba(0, 0, 0, 0.8);
    color: white;
    padding: 5px;
    border-radius: 5px;
    display: none;
  }

  /* initial review */
  .hover-text-1::after {
    content: "Instructor is reviewing the project";
  }

  /* initial revisions */
  .hover-text-2::after {
    content: "Group is revising the document for initial review";
  }

  /* PRF Start */
  .hover-text-3::after {
    content: "Group is filling out the PRF template";
  }

  /* PRF Review */
  .hover-text-4::after {
    content: "Instructor is reviewing PRF for endorsing";
  }

  /* ExD Review */
  .hover-text-5::after {
    content: "Executive director is reviewing the project";
  }

  /* Proofreader Assignment */
  .hover-text-6::after {
    content: "English Office Head is assigning a proofreader";
  }

  /* Proofreading */
  .hover-text-7::after {
    content: "Proofreader is proofreading the document";
  }

  /* Proofreading Revisions */
  .hover-text-8::after {
    content: "Group is revising the document for proofreading";
  }

  /* PRF Completion */
  .hover-text-9::after {
    content: "English Office Head is completing the PRF";
  }

  /* Panel Review */
  .hover-text-10::after {
    content: "Instructor is overseeing final revisions recommended by panelists";
  }

  /* Panel Revisions */
  .hover-text-11::after {
    content: "Group is revising the document for panelist review";
  }

  /* Finalizing */
  .hover-text-12::after {
    content: "Project is being finalized (converting document to PDF)";
  }

  /* Publishing */
  .hover-text-13::after {
    content: "Librarian is reviewing project metadata for publishing";
  }

  /* Published */
  .hover-text-14::after {
    content: "The project is complete";
  }

  /* Add hover styles for all 14 hover-text classes */
  .hover-text:hover::after {
    display: block;
  }
</style>


