@model webapp.Models.ViewModels.StaffListViewModel;
@if (Model.Staff.Count < 1)
{
    <!option>No eligible staff</!option>
}
else
{
    @foreach (var staff in Model.Staff)
    {
        <!option value="@staff.Email"
            @(ViewData["SelectedStaff"] as string == staff.Email ? "selected" : "")
        >@staff.GivenName @staff.LastName</!option>
    }
}


